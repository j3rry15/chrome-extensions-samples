{
  "manifest_version": 3,
  "name": "EF TSE Extension",
  "version": "1.0",
  "description": "Helps TSE to efficiently Troubleshoot",
  "action": {
    "default_title": "Click to open panel"
  },
  "side_panel": {
    "default_path": "sidepanelEntity.html"
  },
  "permissions": ["sidePanel", "tabs", "scripting", "storage", "activeTab"],
  "host_permissions":["http://*/*", "https://*/*"],
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
